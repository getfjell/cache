{
  "name": "@fjell/cache",
  "description": "Cache for Fjell",
  "version": "4.6.6",
  "keywords": [
    "cache",
    "fjell"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"tsc --noEmit --watch\" \"vite build --watch\"",
    "build": "pnpm run lint && tsc --noEmit && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "test": "pnpm run lint && vitest run --coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "dependencies": {
    "@fjell/client-api": "^4.4.6",
    "@fjell/core": "^4.4.7",
    "@fjell/http-api": "^4.4.5",
    "@fjell/logging": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@swc/core": "^1.12.14",
    "@tsconfig/recommended": "^1.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.13.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/cache.git"
  }
}
