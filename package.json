{
  "name": "@fjell/cache",
  "description": "Cache for Fjell",
  "version": "4.6.2",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"tsc --noEmit --watch\" \"vite build --watch\"",
    "build": "pnpm run lint && tsc --noEmit && vite build",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "test": "pnpm run lint && vitest run --coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "dependencies": {
    "@fjell/client-api": "^4.4.2",
    "@fjell/core": "^4.4.2",
    "@fjell/http-api": "^4.4.0",
    "@fjell/logging": "^4.4.2",
    "d3": "^7.9.0",
    "dayjs": "^1.11.13",
    "react": "19.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@swc/core": "^1.11.24",
    "@tsconfig/recommended": "^1.0.8",
    "@types/babel__preset-env": "^7.10.0",
    "@types/d3": "^7.4.3",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node": "^5.0.1",
    "vitest": "^3.1.4"
  },
  "packageManager": "pnpm@10.12.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/cache.git"
  }
}
