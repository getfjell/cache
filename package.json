{
  "name": "@fjell/cache",
  "description": "Cache for Fjell",
  "version": "4.6.19",
  "keywords": [
    "cache",
    "fjell"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"tsc --noEmit --watch\" \"node build.js --watch\"",
    "build": "npm run lint && tsc --noEmit && node build.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "test": "npm run lint && vitest run --coverage",
    "prepublishOnly": "npm run clean && npm run build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "docs:test": "cd docs && npm run test"
  },
  "dependencies": {
    "@fjell/client-api": "file:../fjell-client-api",
    "@fjell/core": "file:../fjell-core",
    "@fjell/http-api": "file:../fjell-http-api",
    "@fjell/logging": "file:../fjell-logging",
    "@fjell/registry": "file:../fjell-registry"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@fjell/eslint-config": "file:../fjell-eslint-config",
    "@swc/core": "^1.13.2",
    "@tsconfig/recommended": "^1.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/cache.git"
  }
}
